<InteractConfig defaultTarget='Main'>
    <target name='Main'>
        <get url='$(BaseUrl)/' />
        <if property='ShouldLogin' value='true'>
            <call target='Login' />
        </if>
        <get url='$(BaseUrl)/data'>
            <output name='title' xpath='//h1' attr='text()' />
        </get>
    </target>
    
    <target name='Login'>
        <get url='$(BaseUrl)/login' />
        <post clientID='login-form'>
            <formValue name='username' value='testuser' />
            <formValue name='password' value='testpass' />
        </post>
    </target>
</InteractConfig>
