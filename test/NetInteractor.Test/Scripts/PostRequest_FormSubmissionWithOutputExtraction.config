<InteractConfig defaultTarget='Main'>
    <target name='Main'>
        <get url='http://localhost/checkout' />
        <post clientID='checkout_form'>
            <formValue name='billing_name' value='Jane Smith' />
            <formValue name='email' value='jane@example.com' />
            <output name='status' xpath='//span[@class="status"]' attr='text()' expectedValue='Success' />
            <output name='customerName' xpath='//span[@class="customer-name"]' attr='text()' />
        </post>
    </target>
</InteractConfig>
