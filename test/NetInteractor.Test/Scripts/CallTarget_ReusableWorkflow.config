<InteractConfig defaultTarget='Main'>
    <target name='GetProductInfo'>
        <get url='http://localhost/products'>
            <output name='productName' xpath='//span[@class="product-name"]' attr='text()' />
            <output name='price' xpath='//span[@class="price"]' attr='text()' />
        </get>
    </target>
    
    <target name='Main'>
        <call target='GetProductInfo' />
        <get url='http://localhost/data'>
            <output name='title' xpath='//h1' attr='text()' />
        </get>
    </target>
</InteractConfig>
